{
    "manifest_version": 2,
    "name": "AccessoryEditorWeb",
    "version": "1.0",
    "description": "Edit Accessories on the web like FFlagAccessoryAdjustmentEnabled5",
    "content_scripts": [
        {
            "matches": ["*://www.roblox.com/*"]
        }
    ],
    "browser_action": {
        "default_popup": "menu.html"
    },
    "permissions": [
        "activeTab",
        "https://avatar.roblox.com/*",
        "https://users.roblox.com/*"
    ]
}
